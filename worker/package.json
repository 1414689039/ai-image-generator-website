{
  "name": "ai-image-generator-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker后端",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --local",
    "dev:remote": "wrangler dev",
    "build": "echo 'No build step required for worker'",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply ai-image-db --local",
    "db:migrate:binding": "wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "wrangler d1 migrations apply ai-image-db",
    "db:create": "wrangler d1 create ai-image-db",
    "db:init": "wrangler d1 execute ai-image-db --local --file=./migrations/0001_initial.sql"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "wrangler": "^4.60.0"
  }
}
